package foo;

import foo.Dog;

public class Person {
	
	private String name1;
	
	private Dog dog;
	
	public void setName(String n){
		name1 = n;
	}
	
	public String getName(){
		return name1;
	}
	
	public Dog getDog(){
		return dog;
	}
	
	public void setDog(Dog dd){
		dog = dd;
		
	}

}
