package foo;

import foo.Dog;

public class Person {
	
	private String name1;
	
	private Dog d;
	
	public void setName(String n){
		name1 = n;
	}
	
	public String getName(){
		return name1;
	}
	
	public Dog getDog(){
		return d;
	}
	
	public void setDog(String n){
		d = new Dog();
		d.setName(n);
		
	}

}
